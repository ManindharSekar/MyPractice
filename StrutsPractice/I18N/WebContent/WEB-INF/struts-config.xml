<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC 
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" 
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>
		<form-bean name="RF" type="bean.AadharForm"/>
	</form-beans>

	<action-mappings>
		<action path="/reg" name="RF" ></action>
		<action path="/changelang" type="bean.ChangeLanguageController">
		<forward name="index" path="/index.jsp"></forward>
		</action>
	</action-mappings>
	<message-resources parameter="resources/AdharForm"></message-resources>
</struts-config>

